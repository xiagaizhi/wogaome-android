<?xml version="1.0"?>
<template
    format="5"
    revision="3"
    name="MVP ListPage"
    description="Creates a new custom view that extends android.view.View and exposes custom attributes.">
    
    <category value="MVP" />
    
    
    <parameter
    id="objectKind"
    name="Page Name"
    type="string"
    constraints="class|unique|nonempty"
    default="MyPage"
    help="By" />
    <parameter
    id="objectTitle"
    name="Page Tilte"
    type="string"
    constraints="nonempty"
    default="标题"
    help="By" />
    
    <parameter
    id="PackageId"
    name="Package Name"
    type="string"
    constraints="nonempty"
    default="com.yushi.leke"
    help="By" />



    <globals file="globals.xml.ftl" />
    <execute file="recipe.xml.ftl" />
    
</template>
